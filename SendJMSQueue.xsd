  <xsd:element name="sendJMSMessage">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="messageContent" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="sendJMSMessageResponse">
