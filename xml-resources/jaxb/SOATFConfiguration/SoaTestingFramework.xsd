<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:common="http://www.archenroot.org/SoaTestingFramework/Common"
           xmlns:dbconf="http://www.archenroot.org/SoaTestingFramework/DatabaseConfiguration"
           xmlns:fileconf="http://www.archenroot.org/SoaTestingFramework/FileConfiguation"
           xmlns:ftpconf="http://www.archenroot.org/SoaTestingFramework/FtpConfiguration"
           xmlns:globconf="http://www.archenroot.org/SoaTestingFramework/GlobalConfiguration"
           xmlns:jmsconf="http://www.archenroot.org/SoaTestingFramework/JmsConfiguration"
           xmlns:mapconf="http://www.archenroot.org/SoaTestingFramework/MappingConfiguration"
           xmlns:osbconf="http://www.archenroot.org/SoaTestingFramework/OsbConfiguration"
           xmlns:soapconf="http://www.archenroot.org/SoaTestingFramework/SoapConfiguration"
           xmlns:stf="http://www.archenroot.org/SoaTestingFramework"
           xmlns:toolconf="http://www.archenroot.org/SoaTestingFramework/ToolConfiguration"
           xmlns:xmlconf="http://www.archenroot.org/SoaTestingFramework/XmlConfiguration"
           targetNamespace="http://www.archenroot.org/SoaTestingFramework"
           elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/Common" schemaLocation="/common/ComnmonTypes.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/DatabaseConfiguration" schemaLocation="/database/DatabaseConfigurationType.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/FileConfiguation" schemaLocation="/file/FileConfigurationType.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/FtpConfiguration" schemaLocation="/ftp/FtpConfigurationType.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/GlobalConfiguration" schemaLocation="/global/GlobalConfigurationType.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/JmsConfiguration" schemaLocation="/jms/JmsConfigurationType.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/MappingConfiguration" schemaLocation="/mappping/MappingConfigurationType.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/OsbConfiguration" schemaLocation="/osb/OsbConfigurationType.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/SoapConfiguration" schemaLocation="/soap/SoapConfigurationType.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/ToolConfiguration" schemaLocation="tool/ToolConfigurationType.xsd"/>
    <xs:import namespace="http://www.archenroot.org/SoaTestingFramework/XmlConfiguration" schemaLocation="/xml/XmlConfigurationType.xsd"/>
    
    
    
    
    <xs:element name="soaTestingFramework" type="SoaTestingFrameworkType"/>
        
    <xs:complexType name="SoaTestingFrameworkType">
        <xs:sequence>
            <xs:annotation>
                <xs:appinfo></xs:appinfo>
                <xs:documentation></xs:documentation>
            </xs:annotation>
            <xs:element name="globalConfiguration" type="globconf:GlobalConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="databaseConfiguration" type="dbconf:DatabaseConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="fileConfiguation" type="fileconf:FileConfiguationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ftpConfiguration" type="ftpconf:FtpConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="jmsConfiguration" type="jmsconf:JmsConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="mappingConfiguration" type="mapconf:MappingConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="osbConfiguration" type="osbconf:OsbConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="soapConfiguration" type="soapconf:SoapConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="toolConfiguration" type="toolconf:ToolConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="xmlConfiguration" type="xmlconf:XmlConfigurationType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
