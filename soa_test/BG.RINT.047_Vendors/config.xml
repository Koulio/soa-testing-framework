    xsi:schemaLocation="http://www.ibm.com/SOATF/Config/Iface ../../SOATestingFramework/schema/SOATFIfaceConfig/SOATFIfaceConfig.xsd">
    <stfconf:ifaceEndPoints>
        <stfconf:ifaceEndPoint endPointLocalId="Sites EBS_PCM_002 Source Database - Valid Data">
            <stfconf:database refId="eis/ebs/DatabaseAdapter">
                <dbconf:dbObjects>
                    <dbconf:dbObject name="XXXIW_MW_PO_VENDOR_SITES_EXT">
                        <dbconf:customValue columnName="INT_STATUS" columnValue="V" pooledValue="S"/>
                        <dbconf:customValue columnName="SEQUENCE_NUM" columnValue="999999"/>
                        <dbconf:customValue columnName="ACTION_FLAG" columnValue="I"/>
                        <dbconf:customValue columnName="VENDOR_ID" columnValue="5004"/>
                        <dbconf:customValue columnName="VENDOR_SITE_ID" columnValue="5011"/>
                        <dbconf:customValue columnName="VENDOR_SITE_CODE" columnValue="Arup Consulting Engineers - Cork"/>
