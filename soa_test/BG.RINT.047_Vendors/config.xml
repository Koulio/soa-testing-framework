                </dbconf:dbObjects>
            </stfconf:database>
        </stfconf:ifaceEndPoint>
        <stfconf:ifaceEndPoint endPointLocalId="Sites EBS_PCM_002 Preprocessing Queue">
            <stfconf:jmsSubsystem>
                <jmsconf:jmsServer name="IWCommonJMSServer"/>
                <jmsconf:connectionFactory name="iw.XAConnectionFactory" jndiName="iw/XAConnectionFactory"/>
                <jmsconf:queue name="iw.Vendors.PreMaintainQueue047V" jndiName="iw/Vendors/PreMaintainQueue047V"/>
                <jmsconf:messageSchema path="../osb/VendorMaintainFromEBS/Resources/Schema/VendorMaintainPollingPS_table.xsd"/>
            </stfconf:jmsSubsystem>
    </stfconf:ifaceEndPoint>
        <stfconf:ifaceEndPoint endPointLocalId="Sites EBS_PCM_002 Maintain Queue">
            <stfconf:jmsSubsystem>
                <jmsconf:jmsServer name="IWCommonJMSServer"/>
                <jmsconf:connectionFactory name="bge.XAConnectionFactory" jndiName="bge/XAConnectionFactory"/>
                <jmsconf:queue name="bge.Vendors.MaintainQueue" jndiName="bge/Vendors/MaintainQueue"/>
                <jmsconf:messageSchema path="../osb/VendorMaintainFromEBS/Resources/Schema/VendorsSitesAndContacts_201006.xsd"/>
            </stfconf:jmsSubsystem>
        </stfconf:ifaceEndPoint>
        <stfconf:ifaceEndPoint endPointLocalId="Sites EBS_PCM_002 Preprocessing Queue Listener Proxy">
            <stfconf:soap>
                <soapconf:serviceName>VendorMaintainFromEBSPS</soapconf:serviceName>
                <soapconf:endPointUri>VendorMaintainFromEBS/Proxy Services/VendorMaintainFromEBSPS</soapconf:endPointUri>
                <soapconf:serviceLocationType>internal</soapconf:serviceLocationType>
                <soapconf:serviceSOAType>ProxyService</soapconf:serviceSOAType>
            </stfconf:soap>
        </stfconf:ifaceEndPoint>
        <stfconf:ifaceEndPoint endPointLocalId="Sites EBS_PCM_002 Maintain Queue Listener Proxy">
            <stfconf:soap>
                <soapconf:serviceName>VendorsSitesAndContactsPS</soapconf:serviceName>
                <soapconf:endPointUri>VendorToMaximoPrimavera/ProxyServices/VendorsSitesAndContactsPS</soapconf:endPointUri>
                <soapconf:serviceLocationType>internal</soapconf:serviceLocationType>
                <soapconf:serviceSOAType>ProxyService</soapconf:serviceSOAType>
            </stfconf:soap>
        </stfconf:ifaceEndPoint>
    </stfconf:ifaceEndPoints>
    <stfconf:utilConfig>
    <utilconf:delays>
        <utilconf:waitForDbPool>30000</utilconf:waitForDbPool>
        <utilconf:waitForQueueMsgTransfer>7000</utilconf:waitForQueueMsgTransfer>
        <utilconf:waitForErrorQueue>30000</utilconf:waitForErrorQueue>
        <utilconf:waitForFTPPool>30000</utilconf:waitForFTPPool>
    </utilconf:delays>
     </stfconf:utilConfig>
   <stfconf:ifaceFlowPatternConfig>
       <stfconf:ifaceFlowPattern refId="Database to Queue to Queue to Database or SOAP" testName="Test case scenario for Vendor Sites of type EBS_PCM_002">
           <stfconf:ifaceTestScenario refId="Positive scenario 1 - Source to Destination">
               <stfconf:ifaceExecBlock refId="Database to Pre-Queue">
                   <stfconf:source endPointLocalIdRef="Sites EBS_PCM_002 Source Database - Valid Data"/>
                   <stfconf:target endPointLocalIdRef="Sites EBS_PCM_002 Preprocessing Queue"/>
                   <stfconf:target endPointLocalIdRef="Sites EBS_PCM_002 Preprocessing Queue Listener Proxy"/>
               </stfconf:ifaceExecBlock>
               <stfconf:ifaceExecBlock refId="Pre-Queue to Maintain Queue">
                   <stfconf:source endPointLocalIdRef="Sites EBS_PCM_002 Source Database - Valid Data"/>
                   <stfconf:source endPointLocalIdRef="Sites EBS_PCM_002 Preprocessing Queue Listener Proxy"/>
                   <stfconf:target endPointLocalIdRef="Sites EBS_PCM_002 Maintain Queue"/>
                   <stfconf:target endPointLocalIdRef="Sites EBS_PCM_002 Maintain Queue Listener Proxy"/>
               </stfconf:ifaceExecBlock>
               <stfconf:ifaceExecBlock refId="Maintain Queue to Database or SOAP Service">
                   <stfconf:source endPointLocalIdRef="Sites EBS_PCM_002 Source Database - Valid Data"/>
                   <stfconf:source endPointLocalIdRef="Sites EBS_PCM_002 Maintain Queue Listener Proxy"/>
               </stfconf:ifaceExecBlock>
            </stfconf:ifaceTestScenario>
           <stfconf:ifaceTestScenario refId="Negative scenario 1 - Database to Pre-Queue with Wrong Data">
               <stfconf:ifaceExecBlock refId="Database to Pre-Queue">
                   <stfconf:source endPointLocalIdRef="Sites EBS_PCM_002 Source Database - Invalid Data"/>
               </stfconf:ifaceExecBlock>
           </stfconf:ifaceTestScenario>
       </stfconf:ifaceFlowPattern>
   </stfconf:ifaceFlowPatternConfig>
</stfconf:soaTFIfaceConfig>        

