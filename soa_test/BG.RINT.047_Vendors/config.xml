                </dbconf:dbObjects>
            </stfconf:database>
        </stfconf:ifaceEndPoint>
        <stfconf:ifaceEndPoint endPointLocalId="Sites EBS_PCM_002 Source Database - Invalid Data">
            <stfconf:database refId="eis/ebs/DatabaseAdapter">
                <dbconf:dbObjects>
                    <dbconf:dbObject name="XXXIW_MW_PO_VENDOR_SITES_EXT">
                        <dbconf:customValue columnName="INT_STATUS" columnValue="V" pooledValue="S"/>
                        <dbconf:customValue columnName="SEQUENCE_NUM" columnValue="999999"/>
                        <dbconf:customValue columnName="ACTION_FLAG" columnValue="I"/>
                        <dbconf:customValue columnName="VENDOR_ID" columnValue="5004"/>
                        <dbconf:customValue columnName="VENDOR_SITE_ID" columnValue="5011"/>
                        <dbconf:customValue columnName="VENDOR_SITE_CODE" columnValue="Arup Consulting Engineers - Cork"/>
                        <dbconf:customValue columnName="VENDOR_CODE" columnValue="13"/>
                        <!-- <dbconf:customValue columnName="FOB_CODE" columnValue="null"/>  -->
                        <!-- <dbconf:customValue columnName="FREIGHT_TERMS" columnValue="null"/> -->
                        <dbconf:customValue columnName="CURRENCY_CODE" columnValue="EUR"/>
                        <!-- framework doesn't support this value, can't convert it <dbconf:customValue columnName="INACTIVE_DATE" columnValue="18-NOV-13"/> -->
                        <dbconf:customValue columnName="ADDRESS1" columnValue="Valovice 9"/>
                        <dbconf:customValue columnName="ADDRESS2" columnValue="Katusice"/>
                        <dbconf:customValue columnName="ADDRESS3" columnValue="Alfa"/>
                        <dbconf:customValue columnName="CITY" columnValue="Mlada Boleslav"/>
                        <dbconf:customValue columnName="STATE" columnValue="Stredni Cechy"/>
                        <dbconf:customValue columnName="PROVINCE" columnValue="Stredni Cechy"/>
                        <dbconf:customValue columnName="MW_INT_NAME" columnValue="EBS_PCM_XXX"/>
